using System;
using System.Collections.Generic;

class WorkerOfferService
{
    private cache<Offer>;
    
    public isValidItem(Offer item)
    {
        return (item.GetMessage() != null && item.GetFee() != null && 
                item.GetTime() != null && item.GetExpirationTime() != null);
    }
    
    public bool add(Offer item)
    {
        if (isValidItem(item))
            return cache.add(item);
        
        return false;
    }
    
    public bool update(Offer new_item, Offer old_item)
    {
        if(isValidItem(item))
            return cache.update(item);
        
        return false;
    }
    
    public bool delete(Offer item)
    {
        if(isValidItem(item))
            return cache.delete(item);
        
        return false;
    }
    
    public List<Offer> get_data()
    {
        return cache.get_date();
    }
}
